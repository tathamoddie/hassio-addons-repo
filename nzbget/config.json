{
  "name": "NZBGet",
  "version": "latest",
  "slug": "nzbget",
  "description": "NZBGet add-on based on the prebuilt docker image from linuxserver",
  "url": "https://nzbget.net/",
  "startup": "services",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "i386"
  ],
  "panel_icon": "mdi:television",
  "ports": {
    "6789/tcp": 6789
  },
  "map": [
    "backup:rw",
    "share:rw",
    "ssl"
  ],
  "webui": "http://[HOST]:[PORT:6789]",
  "boot": "auto",
  "environment": {
    "PUID": "0",
    "PGID": "0"
  },
  "options": {},
  "schema": {}
}